        jQuery(document).ready(function ($) {
            var doSth = function () {







                    $.ajax({
                        type: 'Get',
                        url: '@Url.Action("../Home/realnotify")',
                        dataType: 'json',
                        data: {},   
                        success: function (data) {
                            
                            $("#nbNotif2").html(data);
                            $("#nbNotif").html(data);

                           
                        },
                        error: function (ex) {

                            var r = jQuery.parseJSON(ex.responseText);
                            alert("Message: " + r.Message);
                            alert("StackTrace: " + r.StackTrace);
                            alert("ExceptionType: " + r.ExceptionType);
                        }
                    });

                }
                setInterval(doSth, 2000);//1000 is miliseconds



        });
            </script>

